all:
  children:
    gitlab:
      hosts:
        "${hostname}":
          ansible_host: "${public_ip}"
  vars:
    gitlab_external_url: "${gitlab_external_url}"
    ansible_user:  ubuntu
    ansible_ssh_private_key_file: ~/.ssh/id_rsa
    gitlab_letsencrypt_enable: true
    gitlab_letsencrypt_contact_emails: ["you.mail@gmail.com"]
